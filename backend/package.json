{
  "name": "polymarket-nba-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "migration:run": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infra/db/data-source.ts migration:run",
    "migration:show": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infra/db/data-source.ts migration:show",
    "nba:sync:range": "node -r ts-node/register -r tsconfig-paths/register src/scripts/nba-sync-range.ts",
    "nba:sync:injury-report": "node -r ts-node/register -r tsconfig-paths/register src/scripts/nba-sync-injury-report.ts",
    "polymarket:sync": "node -r ts-node/register -r tsconfig-paths/register src/scripts/polymarket-sync.ts"
  },
  "dependencies": {
    "@coinbase/x402": "^2.1.0",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@x402/core": "^2.3.0",
    "@x402/evm": "^2.3.0",
    "@x402/express": "^2.3.0",
    "bullmq": "^5.12.1",
    "cookie": "^0.6.0",
    "openai": "^6.18.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
